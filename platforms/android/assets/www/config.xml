    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.nycgenerationtech.chromart"
    versionCode = "10" 
    version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>chromART</name>
